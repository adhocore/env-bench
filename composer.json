{
    "name": "adhocore/env-bench",
    "description": "Benchmark some of ENV loaders/parsers for PHP",
    "type": "library",
    "keywords": ["php","env-bench","env","php-env","env-benchmark"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jitendra Adhikari",
            "email": "jiten.adhikary@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ahc\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Ahc\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "adhocore/env": "@stable",
        "vlucas/phpdotenv": "@stable",
        "symfony/dotenv": "@stable",
        "phpbench/phpbench": "@stable"
    },
    "require-dev": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "bench": "phpbench run src --report aggregate"
    }
}
